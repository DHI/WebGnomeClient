{
  "name": "WebGnomeClient",
  "main": "index.html",
  "scripts": {
    "start": "http-server",
    "postinstall": "node ./scripts/postupdate.js",
    "test": "node ./scripts/test.js"
  },
  "config": {
    "localhost,0.0.0.0": {
      "port": "8080",
      "api": "http://hazweb2.orr.noaa.gov:7450",
